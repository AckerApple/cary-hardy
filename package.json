  "bugs": {
    "url": "https://github.com/AckerApple/cary-hardy/issues"
  },
  "homepage": "https://github.com/AckerApple/cary-hardy#readme",
  "devDependencies": {
    "ack-reload": "^4.0.4"
  },
  "dependencies": {
    "taggedjs": "^0.0.12"
  }
}
